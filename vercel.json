{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/beneficiaire"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "installCommand": "npm install --legacy-peer-deps",
  "buildCommand": "npm run build",
  "outputDirectory": "dist/carte-verification",
  "framework": "angular",
  "functions": {
    "api/beneficiaire.js": {
      "maxDuration": 10
    }
  }
}